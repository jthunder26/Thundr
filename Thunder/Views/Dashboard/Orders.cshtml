@{
    ViewBag.Title = "Orders";
    ViewBag.NavbarPartial = "_SideNavbar";
}
<html data-wf-page="643764bbb5653e71abaa0a41" data-wf-site="640dfa53ba67521ab709aa29">

<body data-title="Orders">
    <div class="section dashboard">
        <h1 class="heading-3">Orders</h1>
        <div class="labelcontainer _01">
           
            <div class="feature-image-holder fade-in-move-on-scroll">
                <div data-bind="foreach: labels">
                    <div class="labellinkcard">
                        <div class="columns-16 w-row">
                            <div class="w-col w-col-3">
                                <div class="text-block-20" data-bind="text: labelName"></div>
                                <div class="text-block-21" data-bind="text: dateCreated"></div>
                            </div>
                            <div class="w-col w-col-3">
                                <div class="text-block-22">Complete</div>
                            </div>
                            <div class="column-14 w-col w-col-6">
                                <a data-bind="attr: { href: '/Home/ViewLabel?orderID=' + id, target: '_blank' }" class="link">View</a>
                                <div id="w-node-_08c1b4a6-b00d-afae-e434-e8def79d3c61-abaa0a41" class="line-gradient"></div>
                                <a data-bind="attr: { href: '/Home/DownloadLabel?orderID=' + id, download: 'ShippingLabel.pdf' }" class="link-2">Download</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ko if: hasNoLabel -->
                <div class="w-layout-grid grid-6-col">
                    <div id="w-node-_08c1b4a6-b00d-afae-e434-e8def79d3c65-abaa0a41" class="content">
                       
                            <h2 class="heading-2">Create your first label!</h2>
                        
                       
                            <p class="paragraph-5">Navigate over to the <strong>Ship </strong>tab and begin creating your first label! Once you purchase a label, you can view and download your labels in the <strong>Orders </strong>tab anytime!</p>
                        
                        <div class="list-container">
                            <ul role="list" class="list-with-icons w-list-unstyled">
                                <li class="list-item-with-icon">
                                    <div>All in one platform.</div>
                                </li>
                                <li class="list-item-with-icon">
                                    <div>Fully secured.</div>
                                </li>
                                <li class="list-item-with-icon">
                                    <div>No hidden fees.</div>
                                </li>
                                <li class="list-item-with-icon">
                                    <div>Ship with ease.</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div id="w-node-_08c1b4a6-b00d-afae-e434-e8def79d3c80-abaa0a41" class="image-container"><img src="~/images/Feature-Image-01.svg" loading="lazy" alt="" class="feature-image"></div>
                </div>
                <!-- /ko -->
                
            </div>
        </div>
    </div>
    </body>
</html>